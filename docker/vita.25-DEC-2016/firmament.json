[
  {
    "name"          : "vita-strongloop",
    "Image"         : "jreeme/vita:06-MAR-2017",
    "DockerFilePath": ".",
    "Hostname"      : "vita-strongloop",
    "ExposedPorts"  : {
      "8080/tcp": {},
      "7001/tcp": {}
    },
    "Volumes":{
      "/home/vita/config":{},
      "/home/vita/input-data":{}
    },
    "HostConfig"    : {
      "Binds"       : [
        "/home/jreeme/src/vita-deploy:/home/vita/config",
        "/home/jreeme/watch:/home/vita/input-data"
      ],
      "PortBindings": {
        "7001/tcp" : [
          {
            "HostIp": "192.168.96.152",
            "HostPort": "7001"
          }
        ],
        "8080/tcp" : [
          {
            "HostIp": "192.168.96.152",
            "HostPort": "8080"
          }
        ],
        "8701/tcp" : [
          {
            "HostPort": "8701"
          }
        ],
        "27017/tcp": [
          {
            "HostIp": "192.168.96.152",
            "HostPort": "27017"
          }
        ]
      }
    },
    "ExpressApps"   : [
      {
        "GitUrl"              : "https://github.com/Sotera/loopback-webpack-typescript-amino.git",
        "DeployExisting"      : true,
        "GitSrcBranchName"    : "vita3",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl" : "http://localhost:8701",
        "ServiceName"         : "vita",
        "DoBowerInstall"      : false,
        "ClusterSize"         : 1,
        "EnvironmentVariables": {
          "PORT"              : 8080,
          "NODE_ENV"          : "production",
          "CONFIG_FILE_FOLDER": "/home/vita/config"
        },
        "Scripts"             : [
        ]
      }
    ]
  }
]
