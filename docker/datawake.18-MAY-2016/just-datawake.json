[
  {
    "name"          : "datawake",
    "Image"         : "jreeme/datawake:rancor-stanner",
    "DockerFilePath": "",
    "Hostname"      : "datawake",
    "ExposedPorts"  : {
      "27017/tcp": {},
      "3002/tcp" : {},
      "8182/tcp" : {},
      "8082/tcp" : {},
      "8701/tcp" : {}
    },
    "HostConfig"    : {
      "Binds": [
        "/tmp:/tmp"
      ],
      "PortBindings": {
        "8182/tcp": [
          {
            "HostPort": "8182"
          }
        ],
        "8082/tcp": [
          {
            "HostPort": "8082"
          }
        ],
        "3002/tcp": [
          {
            "HostPort": "3002"
          }
        ],
        "8701/tcp": [
          {
            "HostPort": "8701"
          }
        ]
      }
    },
    "ExpressApps"   : [
      {
        "GitUrl"              : "https://github.com/Sotera/DatawakeDepot",
        "GitSrcBranchName"    : "centcom",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl" : "http://localhost:8701",
        "ServiceName"         : "Datawake",
        "DoBowerInstall"      : true,
        "ClusterSize"         : 1,
        "EnvironmentVariables": {
          "PORT"    : 8082,
          "NODE_ENV": "development"
        },
        "Scripts"             : [
          {
            "StopDeployOnFailure": false,
            "RelativeWorkingDir" : "./browser-plugins/firefox/",
            "Command"            : "/bin/sh",
            "Args"               : [
              "_runFF.sh"
            ]
          }
        ]
      }
    ]
  }
]
